//A
#include<stdio.h>
int main()
{
    int sum[1000];
    int a=0,b=0;
    int i=0,j=0;
    while((scanf("%d %d",&a,&b))==2)
    {
        sum[i]=a+b;
        i++;
    }
    for(j=0;j<i;j++)
    {
        printf("%d\n",sum[j]);
    }
	return 0;
}
//B
#include<stdio.h>
int main()
{
    int sum(int n);
    int n[1000];
    int out[1000];
    int i=0,j=0;
   while((scanf("%d",&n[i]))==1)
   { 
       out[i]=sum(n[i]);
       i++;
   }
   for(j=0;j<i;j++)
   {
       printf("%d\n",out[j]);
   }
	return 0;
}
//D
int sum(int n)
{
    int sum=0;
    if(n%2==0)
    {
        sum=(n+1)*n/2;
    }else
    {
        sum=(n+1)*(n-1)/2+(n+1)/2;
    }
    return sum;
}
//F
#include<stdio.h>
int main()
{
    int x=0;
    int n=0;
    int d=1;
   
    scanf("%d %d %d",&x,&n,&d);
    int i=0;
    int sign=1;
    for(i=0;i<n;i++)
    {
        sign=x*sign;
    }
    
    int t=0;
    t=sign%d;
    printf("%d",t);
    return 0;
}
//C
#include<stdio.h>
int main() {
    char s1[100], s2[100];
    scanf("%s %s", s1, s2);
    int lena, lenb;
    lena = strlen(s1);
    lenb = strlen(s2);
    
    int max=lena;
    if(max<lenb)
    {max=lenb;}
    
    char a[max+1], b[max+1];

    int i = 0, j = 0;
    for(i=0;i<max+1;i++)
    {
        a[i]=0;
        b[i]=0;
    }

    for(i = lena-1; i >= 0; i--)
    {
        a[i] = s1[j] - '0';
        j++;
    }
    // for(i=max;i>max+1-lena;i--)
    // {
    //     a[i]=0;
    // }
    j=0;
    for(i=lenb-1;i>=0;i--)
    {
        b[i]=s2[j] - '0';
        j++;
        
    }
    // for(i=max;i>max+1-lenb;i--)
    // {
    //     b[i]=0;
    // }
    for(i=0;i<max+1;i++)
    {printf("%d ",b[i]);}

    char sum[max+1];

    for(i=0;i<max+1;i++)
    {
        sum[i]=0;
    }
    for(i=0;i<max+1;i++)
    {
        sum[i]=sum[i]+a[i]+b[i];
        if(sum[i]>9)
        {
            sum[i]=sum[i]%10;
            sum[i+1]=1;
        }
    }
    if(sum[max]==0)
    {
        for(i=max-1;i>=0;i--)
        {
            printf("%d",sum[i]);
        }
    }else
    {
        for(i=max;i>=0;i--)
        {
            printf("%d",sum[i]);
        }
    }
    return 0;
}


